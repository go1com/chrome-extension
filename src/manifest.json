{
  "manifest_version": 2,
  "name": "${PRODUCT_NAME}",
  "description": "${PRODUCT_DESCRIPTION}",
  "version": "${PRODUCT_VERSION}",
  "backup_browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "index.html"
  },
  "browser_action": {
    "title": "${PRODUCT_NAME}"
  },
  "icons": {
    "32": "assets/icon_32.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon.png"
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "contextMenus"
  ],
  "options_page": "index.html",
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "polyfills.dll.js",
        "vendor.dll.js",
        "manifest.js",
        "polyfills.js",
        "vendor.js",
        "libs/jquery-3.2.1.min.js",
        "libs/jquery-ui.js",
        "libs/jquery.highlight/jquery.highlight.js",
        "libs/jquery.highlight/jquery.mark.js",
        "libs/jquery.dotdotdot.min.js",
        "contentScript.js"
      ],
      "html": [
        "index.html"
      ],
      "css": [
        "styles/go1ext_inject.css"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "injects/*",
    "assets/fontawesome/*",
    "assets/*",
    "styles/*",
    "libs/*",
    "index.html"
  ]
}
