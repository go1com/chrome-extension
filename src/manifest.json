{
  "manifest_version": 2,
  "name": "GO1 bookmark",
  "description": "GO1 bookmark",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": true,
    "page": "background/background.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "polyfills.dll.js",
        "vendor.dll.js",
        "manifest.js",
        "polyfills.js",
        "injects/helpers.js",
        "injects/content-script.js"
      ],
      "css": [
        "styles/style.css"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    "injects/*"
  ]
}
